<?xml version="1.0" encoding="UTF-8"?>

<module configVersion="1.2">

	<!-- Module Properties -->
	<id>isanteplusconceptmetadata</id>
	<name>iSantePlus Concept Metadata Module</name>
	<version>1.0-SNAPSHOT</version>
	<package>org.openmrs.module.isanteplusconceptmetadata</package>
	<author>Panoramica</author>
	<description>
		Creates the metadata needed for iSantePlus concept dictionary
	</description>
	<updateURL>https://modules.openmrs.org/modules/download/isanteplusconceptmetadata/update.rdf</updateURL>
	<require_version>2.0.5</require_version>
	<!-- / Module Properties -->

	
	<!-- Required Modules -->
	<require_modules>
																											
